<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="75dp"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context=".Preferanse">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <Button
            android:id="@+id/buttonAvbryt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tilbake" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">



        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/svensk"
            android:id="@+id/knappsv"></Button>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/norsk"
            android:id="@+id/knappno"></Button>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/de"
            android:id="@+id/knappde"></Button>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/app_name"/>

        <EditText
            android:id="@+id/antallSp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/knappde"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:hint="Please Write Your 5/10/15"
            android:inputType="number"
            android:textColor="@color/design_default_color_primary"
            android:textSize="25sp" />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="antall sporsmÃ¥l"
            android:id="@+id/buttonPref"></Button>





    </LinearLayout>

</LinearLayout>